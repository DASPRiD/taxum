{
  "name": "@taxum/fs",
  "version": "0.2.0",
  "type": "module",
  "author": "Ben Scholzen 'DASPRiD'",
  "license": "BSD-3-Clause",
  "keywords": [
    "node",
    "typescript",
    "server",
    "http",
    "filesystem"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dasprid/taxum.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "tsx -C taxum --test --test-reporter=spec --experimental-test-module-mocks",
    "test:ci": "c8 --reporter=lcov pnpm test",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": [
        "./dist/index.d.ts",
        "./src/index.ts"
      ],
      "taxum": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@taxum/core": "workspace:*",
    "mime": "^4.0.7",
    "range-parser": "^1.2.1"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/range-parser": "^1.2.7"
  }
}
